{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "lib": [
      "ES2020",
      "ES2021",
      "DOM"
    ],
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "types": [
      "react",
      "react-native",
      "expo"
    ]
  },
  "extends": "expo/tsconfig.base"
}
